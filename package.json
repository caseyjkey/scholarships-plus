  "private": true,
  "sideEffects": false,
  "packageManager": "bun",
  "scripts": {
    "build": "bun-run-all --sequential build:*",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
    "build:extension": "bash scripts/package-extension.sh",
    "dev": "bun-run-all --parallel dev:*",
    "dev:remix": "remix dev",
    "dev:server": "cross-env NODE_ENV=development bun run build:server --watch",
    "docker": "docker compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require ./mocks --require dotenv/config ./build/server.js",
    "dev:worker": "bun --watch worker.ts",
    "dev:worker:build": "bun build worker.ts --outdir dist --target node --packages external",
    "worker": "bun dist/worker.js",
    "worker:build": "bun build worker.ts --outdir dist --target node --packages external",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate deploy",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:e2e:dev": "playwright test --ui",
    "test:e2e:run": "playwright test --config=playwright.standalone.config.ts",
    "pretest:e2e:run": "npm run build",
    "typecheck": "tsc && tsc -p cypress",
    "validate": "npm-run-all --parallel \"test -- --run\" lint typecheck test:e2e:run"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
